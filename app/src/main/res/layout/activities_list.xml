<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
	>

	<Button
		android:id="@+id/shine_btn"
		android:layout_width="142dp"
		android:layout_height="143dp"
		android:layout_alignParentTop="true"
		android:layout_centerHorizontal="true"
		android:layout_marginTop="162dp"
		android:background="@drawable/ar_icon" />


	<Button
		android:id="@+id/traffic_btn"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_alignParentBottom="true"
		android:layout_centerHorizontal="true"
		android:layout_marginBottom="24dp"
		android:background="@color/white" />

	<Button
		android:id="@+id/arnavibtn"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_alignParentStart="true"
		android:layout_alignTop="@+id/traffic_btn"
		android:layout_marginStart="27dp"
		android:background="@color/white"
		android:text="" />

	<Button
		android:id="@+id/mapgobtn"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_alignParentEnd="true"
		android:layout_alignParentRight="true"
		android:layout_alignTop="@+id/traffic_btn"
		android:layout_marginEnd="30dp"
		android:layout_marginRight="30dp"
		android:background="@color/white" />

	<Button
		android:id="@+id/bless_btn"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_alignEnd="@+id/mapgobtn"
		android:layout_alignParentBottom="true"
		android:layout_marginBottom="129dp"
		android:background="@color/white" />

</RelativeLayout>

